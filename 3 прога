//  3 прога
#include <iostream>
    using namespace std;
void func(float arr[], int a, int b)
{
    float e = arr[(a + b) / 2];
    int i = a;
    int j = b;
    while (i <= j)
    {
        while (arr[i] < e) i++;
        while (arr[j] > e) j--;
        if (i <= j)
        {
            swap(arr[i], arr[j]);
            i++; j--;
        }
    }
    if (i < b)
        func(arr, i, b);
    if (a < j)
     func(arr, a, j);
}

int main()
{
    int n;
    cin >> n;
    float *arr = new float[n];
    for (int i = 0; i < n; i++)
        cin >> arr[i];
    func(arr, 0, n - 1);
    cout << endl;
    for (int i = n - 1; i >= 0; i--)
        cout << arr[i] << " " ;
    cout << endl ;
return 0;
}
